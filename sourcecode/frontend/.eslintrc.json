{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": false
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "plugins": ["unused-imports"],
    "rules": {
        "no-console": "off",
        "strict": ["error", "global"],
        "curly": "warn",
        "import/no-anonymous-default-export": "off",
        "react-hooks/exhaustive-deps": "off",
        "unused-imports/no-unused-imports": "error"
    }
}
